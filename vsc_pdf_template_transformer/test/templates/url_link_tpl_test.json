{
  "className": "TplDocument",
  "pageMode": "'none'",
  "children": [
    {
      "className": "TplPage",
      "pageFormat": {
        "format": "'letter'"
      },
      "child": {
        "className": "TplUrlLink",
        "destination": "'https://flutter.dev'",
        "child": {
          "className": "TplText",
          "text": "'sample text'",
          "style": {
            "color": "'00FF00'"
          },
          "textAlign": "left",
          "textDirection": "ltr",
          "softWrap": true,
          "tightBounds": true,
          "textScaleFactor": 2,
          "maxLines": 3,
          "overflow": "span"
        }
      }
    }
  ]
}
