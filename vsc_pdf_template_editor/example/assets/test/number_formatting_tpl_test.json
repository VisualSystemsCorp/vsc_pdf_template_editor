{
  "className": "TplDocument",
  "variables": [
    {
      "variable": "locale",
      "expression": "'en-US'"
    },
    {
      "variable": "tz",
      "expression": "'America/Chicago'"
    }
  ],
  "pageMode": "'none'",
  "children": [
    {
      "className": "TplPage",
      "pageFormat": {
        "format": "'letter'"
      },
      "child": {
        "className": "TplColumn",
        "crossAxisAlignment": "'start'",
        "children": [
          {
            "className": "TplText",
            "text": "'formatLongCurrency: ' + formatLongCurrency(data.total_price)"
          },
          {
            "className": "TplText",
            "text": "'formatCurrency: ' + formatCurrency(data.total_price)"
          },
          {
            "className": "TplText",
            "text": "'formatNumber: ' + formatNumber('#,##0.000', '12345678.9139')"
          },
          {
            "className": "TplText",
            "text": "'formatPercent: ' + formatPercent('.3278', 2)"
          }
        ]
      }
    }
  ]
}
